<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="co.kr.gaepan.mapper.my.MyAdoptApplyMapper">
    <insert id="insertApply">
        INSERT INTO
            `gp_pet_adopt_apply`
        SET
            `name` = #{name},
            `hp` = #{hp},
            `reason` = #{reason},
            `addr1` = #{addr1},
            `addr2` = #{addr2},
            `regDate` = NOW();
    </insert>
</mapper>